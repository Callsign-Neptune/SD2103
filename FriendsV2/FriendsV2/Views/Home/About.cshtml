@{
    ViewData["Title"] = "About";
}
﻿
<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Modern Business - Start Bootstrap Template</title>

    <!-- Bootstrap core CSS -->
    <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="css/modern-business.css" rel="stylesheet">

</head>

<body>
    <nav class="navbar fixed-top navbar-expand-lg navbar-dark bg-dark fixed-top">
        <div class="container">
            <a class="navbar-brand" asp-controller="Home" asp-action="Index">Friends on the Block</a>
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">

                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="Discords">Discords</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="Contact">Contact</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownPortfolio" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Builds
                        </a>
                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownPortfolio">
                            <a class="dropdown-item" asp-controller="BuildTables" asp-action="List">Posts</a>
                        </div>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownBlog" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            My Messages
                        </a>
                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownBlog">
                            <a class="dropdown-item" asp-area="" asp-controller="Messages" asp-action="Inbox">Inbox</a>
                            <a class="dropdown-item" asp-area="" asp-controller="Messages" asp-action="NewMessage">Send Messages</a>
                            <a class="dropdown-item" asp-area="" asp-controller="Messages" asp-action="Read">Read</a>
                        </div>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home" asp-action="About">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="BuildUsers" asp-action="ProfileInfo">My Profile</a>
                    </li>
                    <partial name="_LoginPartial" />
                </ul>
            </div>
        </div>
    </nav>
    <!-- Page Content -->
    <div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <h1 class="mt-4 mb-3">
            About
            <small>Friends on the Block</small>
        </h1>

        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="Home.html">Home</a>
            </li>
            <li class="breadcrumb-item active">About</li>
        </ol>

        <!-- Intro Content -->
        <div class="row">
            <div class="col-lg-6">
                <img class="img-fluid rounded mb-4" src="https://i.pcmag.com/imagery/articles/04SIdjmjrtl6uLRIiiijb9a-1.fit_scale.size_1028x578.v1569487694.jpg" alt="">
            </div>
            <div class="col-lg-6">
                <h2>Friends on the Block</h2>
                <p>Welcome all, to the summit of our time on this amazing game! After hundreds of enjoyable hours given to us by the creators of Minecraft itself and other Minecraft players, we wanted to be able to give something back to the community.</p>
                <p>Our goal here is to help the community be able to share their hard work and enjoy browsing the work of others. So show off that fancy castle, caption that cozy cottage, and see if you can find fresh ideas for your own realms!</p>
                <p>If you would like to join our growing community on Discord here is the link. https://discord.com/invite/WYb3GH5 </p>
            </div>
        </div>
        <!-- /.row -->
        <!-- Team Members -->
        <h2>Our Team</h2>

        <div class="row">
            <div class="col-lg-4 mb-4">
                <div class="card h-100 text-center">
                    <img class="card-img-top" src="https://blog.connectedcamps.com/wp-content/uploads/2016/03/FVeLtzS-750x450.jpg" alt="">
                    <div class="card-body">
                        <h4 class="card-title">Braden Freeman</h4>
                        <h6 class="card-subtitle mb-2 text-muted">Creator</h6>
                        <p class="card-text">Hey guys, Braden here coming to you live from atop my 3 story castle in Minecraft. Thanks for taking a look at our site, I hope you're happy.</p>
                    </div>
                    <div class="card-footer">
                        <a href="#">Braden@friends.com</a>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 mb-4">
                <div class="card h-100 text-center">
                    <img class="card-img-top" src="https://blog.connectedcamps.com/wp-content/uploads/2016/06/maxresdefault-750x450.jpg" alt="">
                    <div class="card-body">
                        <h4 class="card-title">Alex Chalmers</h4>
                        <h6 class="card-subtitle mb-2 text-muted">Creator</h6>
                        <p class="card-text">Hello, everyone! I'm Alex, the cottagecore witch baking cakes in her treehouse hidden in the woods. Thanks for stopping by and looking at our site!</p>
                    </div>
                    <div class="card-footer">
                        <a href="#">Alex@friends.com</a>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 mb-4">
                <div class="card h-100 text-center">
                    <img class="card-img-top" src="https://www.tubefilter.com/wp-content/uploads/2014/11/minecraft.jpg" alt="">
                    <div class="card-body">
                        <h4 class="card-title">Randy Morgan</h4>
                        <h6 class="card-subtitle mb-2 text-muted">Creative Talent and Cheerleader</h6>
                        <p class="card-text">Welcome travelers! Enjoy the site that we have come up with and join us online!...P.S...Don't touch the horse!</p>
                    </div>
                    <div class="card-footer">
                        <a href="#">Randy@friends.com</a>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.row -->
        <!-- Our Customers -->
        <h2>Our Friends</h2>
        <div class="row">
            <div class="col-lg-2 col-sm-4 mb-4">
                <img class="img-fluid" src="https://i.guim.co.uk/img/media/c6f7b43fa821d06fe1ab4311e558686529931492/236_140_947_568/500.jpg?quality=85&auto=format&fit=max&s=d0e878606b06ee44babe6a13c23d1a15" alt="">
            </div>
            <div class="col-lg-2 col-sm-4 mb-4">
                <img class="img-fluid" src="https://pmcvariety.files.wordpress.com/2019/02/minecraft-best-year-yet.png?w=681&h=383&crop=1" alt="">
            </div>
            <div class="col-lg-2 col-sm-4 mb-4">
                <img class="img-fluid" src="https://destructoid.com/ul/548022-minecraft%20removes%20notch.jpg" alt="">
            </div>
            <div class="col-lg-2 col-sm-4 mb-4">
                <img class="img-fluid" src="https://sickr.files.wordpress.com/2015/11/minecraft_characters.jpg?w=780" alt="">
            </div>
            <div class="col-lg-2 col-sm-4 mb-4">
                <img class="img-fluid" src="https://cdn.thinglink.me/api/image/830265843840450562/1240/10/scaletowidth" alt="">
            </div>
            <div class="col-lg-2 col-sm-4 mb-4">
                <img class="img-fluid" src="https://www.itl.cat/pngfile/big/145-1451436_minecraft-best-team-minecraft-steve-saga-comics.jpg" alt="">
            </div>

        </div>
        <!-- /.row -->

    </div>
    <!-- /.container -->
    <!-- Bootstrap core JavaScript -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

</body>

</html>
